{
  "name": "vite-plugin-typed-pages",
  "version": "0.0.1",
  "description": "Provide autocompletion for pages route names generated by vite-plugin-pages router",
  "type": "module",
  "scripts": {
    "dev": "npm run prepack -- --watch",
    "prepack": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "yarn prepack && vitest run",
    "test:watch": "yarn prepack && vitest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nuxt typed router",
    "nuxt router",
    "nuxt typed",
    "nuxt safe router",
    "nuxt typed routes",
    "nuxt generate route ts",
    "nuxt 3",
    "nuxt 3 router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://victorgarciaesgi@github.com/victorgarciaesgi/vite-plugin-typed-pages.git"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorgarciaesgi/vite-plugin-typed-pages/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "log-symbols": "^5.1.0",
    "mkdirp": "^1.0.4",
    "pathe": "^0.3.0",
    "prettier": "^2.7.1",
    "vite-plugin-pages": "^0.24.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "cross-env": "^7.0.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "i": "^0.3.7",
    "picocolors": "^1.0.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.15.1",
    "vue-router": "^4.0.16"
  }
}
